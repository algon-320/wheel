fn fib(n: u64) -> u64 {
    if n <= 1 {
        n
    } else {
        let x = 0;
        let y = 1;
        let z = x + y;
        let i = 2;

        loop {
            if i == n {
                break
            };
            x = y;
            y = z;
            z = x + y;
            i += 1;
        };

        z
    }
}

fn main() -> u64 {
    fib(30)
}
