fn fib(n: u64) -> u64 {
    if n <= 1 {
        n
    } else {
        let x = 0 in
        let y = 1 in
        let z = x + y in
        let i = 2 in {
            loop {
                if i == n {
                    break
                } else {
                    x = y;
                    y = z;
                    z = x + y;
                    i = i + 1;
                }
            };
            z
        }
    }
}

fn main() -> u64 {
    fib(30)
}
