fn main() -> u64 {
    let cnt = 0 in
    let a = 0 in {
        loop { // loop A
            if a == 2 {
                break; // loop A
            };
            let b = 0 in
            loop { // loop B
                if b == 5 {
                    break; // loop B
                };
                cnt += 1;
                b += 1;
            };
            a += 1;
        };
        cnt
    }
}
